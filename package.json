{
  "name": "healthcare-project",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\" \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && nodemon -r dotenv/config --experimental-json-modules server.js",
    "start:frontend": "cd frontend && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^3.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1"
  }
}
